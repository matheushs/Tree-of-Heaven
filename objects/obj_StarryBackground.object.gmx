            <string>///Drawing Scrolling Background

var s_w = sprite_get_width(spr_StarrySky);
var s_h = sprite_get_height(spr_StarrySky);

current_x = clamp(current_x - scrolling_speed_h, -s_w, 0);
current_y = clamp(current_y - scrolling_speed_v, -s_h, 0);

var i = 0;
var j = 0;

for(i = 0; i &lt; numberOfImagesV + 1; i++){
    for(j = 0; j &lt; numberOfImagesH + 1; j++){
        draw_sprite(spr_StarrySky, 0, current_x + j*s_w, current_y + i * s_h)
    }
}

if(scrolling_speed_h &gt; 0 &amp;&amp; current_x == -s_w){
    current_x = 0;
}else if(scrolling_speed_h &lt; 0 &amp;&amp; current_x == 0){
    current_x = -s_w;
}

if(scrolling_speed_v &gt; 0 &amp;&amp; current_y == -s_h){
    current_y = 0;
}else if(scrolling_speed_v &lt; 0 &amp;&amp; current_y == 0){
    current_y = -s_w;
}
</string>
